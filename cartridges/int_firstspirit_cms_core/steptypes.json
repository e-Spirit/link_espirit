{
  "step-types": {
    "pipeline-step": [
      {
        "@type-id": "custom.FirstSpirit.ImportLibrary",
        "description": "Import the library content generated by FirstSpirit.",
        "pipeline": "ContentSync-LIBRARY",
        "parameters": {
          "parameter": [
            {
              "@name": "ImportFile",
              "@description": "Location of the import file generated by FirstSpirit, relative to IMPEX/.",
              "@type": "string",
              "@required": "true",
              "@trim": "true"
            },
            {
              "@name": "ImportMode",
              "@description": "Import mode to use.",
              "@type": "string",
              "@required": "true",
              "@trim": "false",
              "enum-values": {
                "value": [
                  "DELETE",
                  "MERGE",
                  "REPLACE",
                  "UPDATE"
                ]
              }
            }
          ]
        }
      },
      {
        "@type-id": "custom.FirstSpirit.ImportSlotConfigurations",
        "description": "Import the slot configurations generated by FirstSpirit.",
        "pipeline": "ContentSync-SLOT_CONFIGURATIONS",
        "parameters": {
          "parameter": [
            {
              "@name": "ImportFile",
              "@description": "Location of the import file generated by FirstSpirit, relative to IMPEX/.",
              "@type": "string",
              "@required": "true",
              "@trim": "true"
            },
            {
              "@name": "ImportMode",
              "@description": "Import mode to use.",
              "@type": "string",
              "@required": "true",
              "@trim": "false",
              "enum-values": {
                "value": [
                  "DELETE",
                  "MERGE",
                  "REPLACE",
                  "UPDATE"
                ]
              }
            }
          ]
        }
      }
    ]
  }
}
